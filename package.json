{
  "name": "letsdo",
  "version": "1.0.0",
  "description": "to-do-list",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
     "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
"package-win": "electron-packager . Let'sDo --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Let's DO\"",    
"package-linux": "electron-packager . Let'sDo --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanjay1708/LetsDo.git"
  },
  "author": "Sanjay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanjay1708/LetsDo/issues"
  },
  "homepage": "https://github.com/sanjay1708/LetsDo#readme",
  "dependencies": {
    "electron": "^11.0.3"
  },
  "devDependencies": {
    "electron-packager": "^15.1.0"
  }
}
